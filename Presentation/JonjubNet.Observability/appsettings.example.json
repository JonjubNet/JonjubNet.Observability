{
  "JonjubNet": {
    "Logging": {
      "Enabled": true,
      "ServiceName": "MyService",
      "Environment": "Development",
      "Version": "1.0.0",
      "RegistryCapacity": 10000,
      "BatchSize": 200,
      "FlushIntervalMs": 1000,
      "MinLevel": "Information",
      "Sinks": {
        "Console": {
          "Enabled": true
        },
        "Elasticsearch": {
          "Enabled": false,
          "BaseUrl": "http://localhost:9200",
          "IndexName": "logs",
          "Username": "",
          "Password": ""
        },
        "Kafka": {
          "Enabled": false,
          "BootstrapServers": "localhost:9092",
          "Topic": "logs",
          "ClientId": "observability-logging"
        },
        "Http": {
          "Enabled": false,
          "Endpoint": "https://api.example.com/logs",
          "TimeoutMs": 30000,
          "Headers": {}
        },
        "OpenTelemetry": {
          "Enabled": false,
          "Endpoint": "http://localhost:4318",
          "Protocol": "HttpProtobuf"
        },
        "Serilog": {
          "Enabled": false
        }
      },
      "Filters": {
        "MinLevel": "Information",
        "MaxLevel": "Critical",
        "ExcludedCategories": [],
        "AllowedCategories": [],
        "ExcludedOperations": [],
        "ExcludedMessagePatterns": []
      },
      "DeadLetterQueue": {
        "Enabled": true,
        "MaxSize": 1000
      },
      "RetryPolicy": {
        "MaxRetries": 3,
        "InitialDelayMs": 1000,
        "MaxDelayMs": 10000,
        "BackoffMultiplier": 2.0
      },
      "CircuitBreaker": {
        "Enabled": true,
        "FailureThreshold": 5,
        "SuccessThreshold": 2,
        "TimeoutMs": 60000
      }
    },
    "Metrics": {
      "Enabled": true,
      "ServiceName": "MyService",
      "Environment": "Development",
      "Version": "1.0.0",
      "QueueCapacity": 10000,
      "BatchSize": 200,
      "FlushIntervalMs": 1000,
      "Mode": "InProcess",
      "Sinks": {
        "Prometheus": {
          "Enabled": true,
          "Port": 9090,
          "Path": "/metrics"
        },
        "InfluxDB": {
          "Enabled": false,
          "Url": "http://localhost:8086",
          "Database": "metrics",
          "Username": "",
          "Password": ""
        },
        "StatsD": {
          "Enabled": false,
          "Host": "localhost",
          "Port": 8125,
          "Prefix": "observability"
        },
        "Kafka": {
          "Enabled": false,
          "BootstrapServers": "localhost:9092",
          "Topic": "metrics",
          "ClientId": "observability-metrics"
        },
        "OpenTelemetry": {
          "Enabled": false,
          "Endpoint": "http://localhost:4318",
          "Protocol": "HttpProtobuf"
        },
        "Elasticsearch": {
          "Enabled": false,
          "BaseUrl": "http://localhost:9200",
          "IndexName": "metrics"
        },
        "Http": {
          "Enabled": false,
          "Endpoint": "https://api.example.com/metrics",
          "TimeoutMs": 30000
        }
      },
      "DeadLetterQueue": {
        "Enabled": true,
        "MaxSize": 1000
      },
      "RetryPolicy": {
        "MaxRetries": 3,
        "InitialDelayMs": 1000,
        "MaxDelayMs": 10000,
        "BackoffMultiplier": 2.0
      },
      "CircuitBreaker": {
        "Enabled": true,
        "FailureThreshold": 5,
        "SuccessThreshold": 2,
        "TimeoutMs": 60000
      }
    },
    "Tracing": {
      "Enabled": true,
      "ServiceName": "MyService",
      "Environment": "Development",
      "Version": "1.0.0",
      "RegistryCapacity": 10000,
      "BatchSize": 200,
      "FlushIntervalMs": 1000,
      "Exporters": {
        "OpenTelemetry": {
          "Enabled": true,
          "Endpoint": "http://localhost:4318",
          "Protocol": "HttpProtobuf"
        },
        "Kafka": {
          "Enabled": false,
          "BootstrapServers": "localhost:9092",
          "Topic": "traces",
          "ClientId": "observability-tracing"
        },
        "Elasticsearch": {
          "Enabled": false,
          "BaseUrl": "http://localhost:9200",
          "IndexName": "traces"
        },
        "Http": {
          "Enabled": false,
          "Endpoint": "https://api.example.com/traces",
          "TimeoutMs": 30000
        }
      },
      "DeadLetterQueue": {
        "Enabled": true,
        "MaxSize": 1000
      },
      "RetryPolicy": {
        "MaxRetries": 3,
        "InitialDelayMs": 1000,
        "MaxDelayMs": 10000,
        "BackoffMultiplier": 2.0
      },
      "Sampling": {
        "Enabled": true,
        "SampleRate": 1.0
      }
    },
    "Observability": {
      "Enabled": true,
      "EnableAutomaticLogging": true,
      "EnableAutomaticMetrics": true,
      "EnableAutomaticTracing": true,
      "SampleRate": 1.0,
      "ExcludedPaths": [
        "/health",
        "/metrics",
        "/favicon.ico"
      ],
      "SanitizePathsForMetrics": true,
      "PathSanitizationPatterns": [
        "/\\d+",
        "/[a-f0-9-]+",
        "/[a-zA-Z0-9]{8,}"
      ],
      "Correlation": {
        "ReadIncomingCorrelationId": true,
        "CorrelationIdHeaderName": "X-Correlation-Id"
      }
    }
  }
}

